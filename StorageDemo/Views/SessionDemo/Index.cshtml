@model Dictionary<string,string >

@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h2>Session data</h2>
    <p>
        @foreach(var kvp in Model)
        {
            <span>@kvp.Key</span>
            <span>=</span>
            <span>@kvp.Value</span>
            <br/>
        }
    </p>

    <h2>Add to session</h2>
    <form asp-action="SetSession" asp-controller="SessionDemo">
        Key : <input name="key" type="text" class="form-control"/><br/>
        Value : <input name="value" type="text" class="form-control" /><br />
        <input type="submit" value="Change" class="btn btn-primary" />
    </form>
</div>